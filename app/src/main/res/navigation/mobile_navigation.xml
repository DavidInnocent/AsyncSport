<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.asynclabs.asyncsport.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.asynclabs.asyncsport.ui.athletes.AthletesFragment"
        android:label="@string/title_athletes"
        tools:layout="@layout/fragment_athletes" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.asynclabs.asyncsport.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" />
    <fragment
        android:id="@+id/paymentFragment"
        android:name="com.asynclabs.asyncsport.PaymentFragment"
        android:label="fragment_payment"
        tools:layout="@layout/fragment_payment" />


</navigation>